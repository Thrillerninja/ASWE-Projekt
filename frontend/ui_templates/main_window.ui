     <string notr="true">/* General Combo Box Style */
QComboBox {
    font-size: 14px;              /* Font size for the text */
    color: #2c3e50;               /* Text color */
    padding: 4px;                 /* Padding inside the combo box */
    border: 2px solid #2980b9;    /* Blue border for the combo box */
    border-radius: 4px;           /* Rounded corners for the combo box */
    background-color: #ecf0f1;    /* Light grey background */
}

/* Arrow Indicator (the dropdown arrow) */
QComboBox::drop-down {
    width: 20px;                  /* Size of the dropdown arrow area */
    border-left: 2px solid #2980b9;/* Left border to separate the arrow */
    background-color: #3498db;    /* Blue background for the dropdown arrow */
}

QComboBox::down-arrow {
    image: url(frontend/ui_templates/down_arrow.png);  /* Custom arrow icon */
    width: 12px;                /* Size of the arrow */
    height: 12px;
}


/* Combo Box when the dropdown is open */
QComboBox QAbstractItemView {
    border: 2px solid #2980b9;   /* Blue border around the dropdown list */
    background-color: #ecf0f1;   /* Light grey background for the dropdown list */
    selection-background-color: #3498db; /* Blue background for selected items */
    selection-color: white;      /* White text for selected items */
}

