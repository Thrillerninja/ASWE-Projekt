    border: 2px solid #2980b9;     /* Blue border */
    border-radius: 4px;            /* Rounded corners */
    background-color: #ecf0f1;     /* Light grey background */
}

/* Time Edit Buttons (Up and Down Arrow Area) */
QTimeEdit::up-button, QTimeEdit::down-button {
    width: 18px;                   /* Size of the button area */
    border: none;                  /* No border */
    background-color: #3498db;     /* Blue background */
}

QTimeEdit::up-arrow {
    width: 10px;                   /* Arrow size */
    height: 10px;
    image: url(frontend/ui_templates/up_arrow.png);  /* Custom arrow icon */
}

QTimeEdit::down-arrow {
    width: 10px;                   /* Arrow size */
    height: 10px;
    image: url(frontend/ui_templates/down_arrow.png);  /* Custom arrow icon */
}

/* Hover Effect for Time Edit */
QTimeEdit:hover {
    border-color: #3498db;         /* Change border color on hover */
}
